    "    hdi_ = az.hdi(np.array(lala))\n",
    "    up.append(hdi_[:,1])\n",
    "    low.append(hdi_[:,0])\n",
    "up = np.concatenate(up)\n",
    "low = np.concatenate(low)"
   ]
